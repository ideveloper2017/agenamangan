{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0.2",
        "ext-curl": "^8.0",
        "ext-gd": "^8.0",
        "ext-json": "^8.0",
        "ext-zip": "^1.19",
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "barryvdh/laravel-translation-manager": "^0.6.3",
        "beyondcode/laravel-query-detector": "^1.6",
        "brian2694/laravel-toastr": "^5.57",
        "cviebrock/eloquent-sluggable": "^9.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^v9.5.0",
        "laravel/helpers": "^1.6",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.3",
        "livewire/livewire": "^2.11",
        "mcamara/laravel-localization": "^1.7",
        "mews/purifier": "^3.3",
        "nwidart/laravel-modules": "^9.0",
        "prettus/l5-repository": "^2.8",
        "rappasoft/laravel-livewire-tables": "^2.10",
        "rezaamini-ir/migrator": "^1.5",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-feed": "^4.2",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-menu": "^4.1",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-query-builder": "^5.1",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/laravel-view-models": "^1.5",
        "spatie/menu": "^3.0",
        "stevebauman/location": "^6.5",
        "yajra/laravel-datatables": "^9.0",
        "yajra/laravel-datatables-oracle": "^10.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\Helpers\\": "Helpers/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
